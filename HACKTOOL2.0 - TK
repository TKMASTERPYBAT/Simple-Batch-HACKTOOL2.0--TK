set /p port="Enter the port number to check: "
echo Checking if port %port% on %host% is open...
powershell -command "Test-NetConnection -ComputerName %host% -Port %port%"
echo.
echo.
echo Press Any Key To Continue...
pause >nul
cls 
color 4
goto Next

